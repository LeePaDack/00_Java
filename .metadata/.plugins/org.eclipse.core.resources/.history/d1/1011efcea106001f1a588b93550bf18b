package com.kh.test.Main;

import java.util.Scanner;

public class Test3 {
	
	public static void main(String[] args) {
		// switch 문 실행해보기
		
		Scanner sc = new Scanner(System.in);
		System.out.print("숫자 작성 : ");
		int date = sc.nextInt();
		String hw;
		
		switch(date) {
		case 1:
			hw = "월요일 : 업무";
			break;
		case 2:
			hw = "화요일 : 공부";
			break;
		default :
			hw = "휴식";
			break
		}
		
		
		
		
		
		
		
		
	}
	
}
