package sup1.model;

public class Score {
// 필드
	int Kor = 0;
	int Eng = 0;
	int Math = 0;
	int total = 0;
	int avg = 0;

	
// 메서드
	// 필수 생성자
	public Score(int kor, int eng, int math, int total, int avg) {
		this.Kor = kor;
		this.Eng = eng;
		this.Math = math;
		this.total = total;
		this.avg = avg;
	}
	
	// void
	
	
	
	
	
	
	
	
	
	
	
}
