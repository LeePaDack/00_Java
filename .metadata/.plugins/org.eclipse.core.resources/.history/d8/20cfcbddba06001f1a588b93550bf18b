package sup1.model;

public class Score {
// 필드
	int kor;
	int eng;
	int math;

	
// 메서드
	// 필수 생성자
	public Score(int kor, int eng, int math, int total, double avg) {
		this.kor = kor;
		this.eng = eng;
		this.math = math;
		this.total = total;
		this.avg = avg;
	}
	
	// void
	public void result() {
		total = kor + eng + math;
		avg = total / 3;
		
		if (avg >= 50) {
			System.out.println("합격");
		}
		else {
			System.out.println("불합격");
		}
		
	}
	
	
	
	
	
	
	
	
	
	
}
