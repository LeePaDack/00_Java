package com.kh.FileEx;
/*
StringBuilder : 속도가 빠름


 */
public class BuilderWriterEx {
	
	public static void main(String[] args) {
		// 파일 생성
		String 파일이름 = "줄.txt";
		
		// StringBuilder 를 사용해서 파일에 쓸 내용 작성
		StringBuilder 빌더 = new StringBuilder();
		빌더.append("첫 번째 줄이어라. \n");
		
		
		
		
		
	}
	
	
}
